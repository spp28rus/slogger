{
    "name": "slogger/laravel",
    "description": "SLogger for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel",
            "email": "sppetrovich@mail.ru"
        }
    ],
    "require": {
        "ext-grpc": "*",
        "ext-protobuf": "*",
        "grpc/grpc": "^1.57",
        "laravel/framework": "^9.0 | ^10.0"
    },
    "autoload": {
        "psr-4": {
            "SLoggerLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SLoggerLaravel\\SLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
